{
  "name": "herbsshelf",
  "version": "1.0.0",
  "description": "A self-generate documentation about the usecases (buchu) of herbsJS",
  "main": "./src/shelf.js",
  "scripts": {
    "test": "mocha --timeout 999999 --colors --recursive ./test",
    "coverage": "nyc --reporter=html mocha ./test",
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix"
  },
  "directories": {
    "lib": "src",
    "test": "test",
    "example": "examples"
  },
  "engines": {
    "node": ">= 12.3.0"
  },
  "keywords": [
    "self-documentation",
    "shelf",
    "herbsshelf",
    "buchu",
    "herbsjs"
  ],
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.3.2",
    "buchu": "^1.0.11",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  },
  "author": "Jhonathan Soares",
  "license": "MIT"
}
